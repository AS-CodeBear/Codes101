#include<stdio.h>

int main()
{
    int N;
    scanf("%d\n",&N);
    int arr[N];
    for(int i=0;i<N;i++){
        int x;
        scanf("%d ",&x);
        arr[i]=x;
    }
    if ((N%2)==0){
        int median=(arr[(N/2)-1]+arr[(N/2)])/2;
        printf("%d",median);
    }
    else if ((N%2)!=0){
        int median=(arr[((N+1)/2)-1]);
        printf("%d",median);
    }
    //  Insert your code here.
    
    return 0;
}
