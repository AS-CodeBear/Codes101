#include<stdio.h>
char way(int n){
    if ((n%2)==0){
        return 'L';
    }
    else if ((n%3)==0){
        return 'R';
    }
    else return 'S';
}


int main()
{
    int n;
    scanf("%d\n",&n);
    for (int i=0;i<n;i++){
        int x;
        scanf("%d ",&x);
        printf("%c ",way(x));
    }
    //  Insert your code here.
    
    return 0;
}
