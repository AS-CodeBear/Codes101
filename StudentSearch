#include<stdio.h>

int main()
{
    int roll,r1,r2,r3,r4;
    scanf("%d",&roll);
    r1=roll-100*(roll/100);
    r2=(roll/100)-100*(roll/10000);
    r3=(roll/10000)-100*(roll/1000000);
    r4=(roll/1000000)-100*(roll/100000000);
    //  Insert y
    char a,b,c,d;
    a= (r4%26)+65;
    b= (r3%26)+65;
    c= (r2%26)+65;
    d= (r1%26)+65;
    printf("%c%c%c%c",a,b,c,d);
    
    
    return 0;
}
