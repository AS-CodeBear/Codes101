#include<stdio.h>
int main(){
    for(int i=1;i<6;i++){
        int k=i-1;
        while(k>0){
        printf(" ");
        k--;
        }
        for(int j=1;j<6;j++){
            if(j==1||j==5||i==5||i==1)printf("*");
            else printf(" ");
        }
        printf("\n");
    }
    
    return 0;
}
