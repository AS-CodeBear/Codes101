#include<stdio.h>

int main()
{
    int n;
    scanf("%d\n",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int minh=arr[0];
    for (int i=1;i<n;i++){
        if (arr[i]<=minh){
            minh=arr[i];
        }
    }
    printf("%d",minh);
    //  Insert your code here.
    
    return 0;
}
