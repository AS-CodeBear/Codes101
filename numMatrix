#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    for(int i=1;i<n+1;i++){
       for(int j=1;j<n+1;j++){
          if(i==1||i==n){
              if(j==1||j==n){
                  printf("0");
              }
              else printf("1");
          }
          else{
              if(j==1||j==n){
                  printf("1");
              }
              else printf("0");
          }
       }    
       printf("\n");
    }
    
    return 0;
}
